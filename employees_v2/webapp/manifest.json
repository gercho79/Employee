{
  "_version": "1.45.0",
  "sap.app": {
    "id": "logaligroup.employeesv2",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.8.1",
      "toolsId": "d37bd9b7-e5e2-4341-a78e-7e5bb8fb4f66"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "northwind.svc": {
        "uri": "/V2/Northwind/Northwind.svc/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/metadata.xml"
        }
      },
      "YSAPUI5_SRV_01": {
        "uri": "/sap/opu/odata/sap/YSAPUI5_SRV_01/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/metadata.xml"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.109.3",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "logaligroup.employeesv2.i18n.i18n"
        }
      },
      "jsonModelM": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "logaligroup.employeesv2.localService.mockdata.Employees.json",
        "dataSource": "jsonManifest"
      },
      "incidentModel": {
        "dataSource": "YSAPUI5_SRV_01",
        "preload": true,
        "settings": {
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Inline",
          "refreshAfterChange": false
        }
      },
      "odataNortwhind": {
        "dataSource": "northwind.svc",
        "preload": true,
        "settings": {
          "defaultCountMode": "Request",
          "defaultOperationMode": "Server",
          "defaultBindingMode": "OneWay",
          "refreshAfterChange": false
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "logaligroup.employeesv2.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": "",
          "target": ["TargetMain"]
        },
        {
          "name": "RouteOrderDetails",
          "pattern": "OrderDetails/{OrderId}",
          "target": ["TargetOrderDetails"]
        }
      ],
      "targets": {
        "TargetMain": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Main",
          "viewName": "Main"
        },
        "TargetOrderDetails": {
          "viewType": "XML",
          "viewName": "OrderDetails"
        }
      }
    },
    "rootView": {
      "viewName": "logaligroup.employeesv2.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
